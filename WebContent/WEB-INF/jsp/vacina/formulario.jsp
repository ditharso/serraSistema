<div align="center">

	<form action="adiciona"  method="post">


		<fieldset>
			<legend>Adicionar Vacina</legend>

			<table border="1">

				<tr>
					<td><label for="nome">Nome:</label></td>
					<td><input id="nome" type="text" name="vacina.nome" /></td>
				</tr>

				<tr>
					<td><label for="fabricante">fabricante:</label></td>
					<td><input id="fabricante" type="text"
						name="vacina.fabricante" /></td>
				</tr>

				<tr>
					<td><label for="validade">validade:</label></td>
					<td><input id="validade" type="text" name="vacina.validade" />
					</td>
				</tr>

				<tr>
					<td><label for="dosagem">Dosagem:</label></td>
					<td><input id="dosagem" type="text" name="vacina.dosagem" />
					</td>
				</tr>

				<tr>
					<td><label for="tipo">tipo:</label></td>
					<td><input id="tipo" type="text" name="vacina.tipo" /></td>
				</tr>

				<tr>
					<td><label for="local">local:</label></td>
					<td><input id="local" type="text" name="vacina.local" /></td>
				</tr>

				<tr>
					<td><label for="contraIndicacao">contraIndicacao:</label></td>
					<td><input id="contraIndicacao" type="text"
						name="vacina.contraIndicacao" /></td>
				</tr>


				<tr>
					<td></td>
					<td>
						<button type="submit">Enviar</button>
					</td>
				</tr>
			</table>
			<br>
		</fieldset>


	</form>

</div>