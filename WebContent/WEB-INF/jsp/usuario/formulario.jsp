
<title>FORMULARIO DE USUARIO!</title>

<body>

	<form action="adiciona" method="post">


		<fieldset>
			<legend>Adicionar Usuario</legend>

			<table border="1">

				<tr>
					<td><label for="nome">Nome:</label></td>
					<td><input id="nome" type="text" name="usuario.nome" /></td>
				</tr>

				<tr>
					<td><label for="login">Login:</label></td>
					<td><input id="login" type="text" name="usuario.login" /></td>
				</tr>

				<tr>
					<td><label for="senha">senha</label></td>
					<td><input id="senha" type="password" name="usuario.senha" /></td>
				</tr>


				<tr>
					<td><label for="sexo">cpf:</label></td>
					<td><input id="sexo" type="text" name="usuario.sexo" /></td>
				</tr>

				<tr>
					<td><label for="nascimento">nascimento:</label></td>
					<td><input id="nascimento" type="text"
						name="usuario.nascimento" /></td>
				</tr>

				<tr>
					<td><label for="cargo">cargo:</label></td>
					<td><input id="cargo" type="text" name="usuario.cargo" /></td>
				</tr>

				<tr>
					<td><label for="cpf">cpf:</label></td>
					<td><input id="cpf" type="text" name="usuario.cpf" /></td>
				</tr>

				<tr>
					<td><label for="estado">estado:</label></td>
					<td><input id="estado" type="text" name="usuario.estado" /></td>
				</tr>

				<tr>
					<td><label for="cidade">cidade:</label></td>
					<td><input id="cidade" type="text" name="usuario.cidade" /></td>
				</tr>

				<tr>
					<td><label for="cep">cep:</label></td>
					<td><input id="cep" type="text" name="usuario.cep" /></td>
				</tr>

				<tr>
					<td><label for="endereco">endereco:</label></td>
					<td><input id="endereco" type="text" name="usuario.endereco" /></td>
				</tr>

				<tr>
					<td><label for="telefone">telefone:</label></td>
					<td><input id="telefone" type="text" name="usuario.telefone" /></td>
				</tr>

				<tr>
					<td><label for="telefone2">telefone2:</label></td>
					<td><input id="telefone2" type="text" name="usuario.telefone2" /></td>
				</tr>

				<tr>
					<td></td>
					<td>
						<button type="submit">Enviar</button>
					</td>
				</tr>

			</table>
			<br />
		</fieldset>


	</form>

</body>
</html>